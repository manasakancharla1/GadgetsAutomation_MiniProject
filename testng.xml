<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="SnapdealSuite">
    <test name="SnapdealTestChrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.selenium.miniproject.GadgetAutomation"/>
        </classes>
    </test>

    <test name="SnapdealTestEdge">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="com.selenium.miniproject.GadgetAutomation"/>
        </classes>
    </test>
</suite>
